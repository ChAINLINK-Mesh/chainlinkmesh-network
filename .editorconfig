root = true

[*]
charset = utf-8
end_of_line = lf
indent_size = 2
indent_style = tab
insert_final_newline = true
max_line_length = 120
tab_width = 2
ij_continuation_indent_size = 2

[*.data]
insert_final_newline = false

[*.sha256]
insert_final_newline = false

[CMakeLists.txt]
indent_size = 4
indent_style = space
